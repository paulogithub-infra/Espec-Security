#!/usr/bin/env groovy
 
pipeline {
    //Agent é o NÓ que vai rodar o job
    agent any
 
    //Fases do pipeline
    stages {
        
       stage('Checkout') {
            steps {
                script {
                    echo 'Fazer o checkout do projeto'
                            url: 'https://github.com/paulonill/Pipeline-groovy'
                }
            }
        }
     }
}
